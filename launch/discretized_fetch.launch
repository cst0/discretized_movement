    <rosparam param="start_pose_joint_states">[0.35             , 0.16          , 0.0            , 0.875           , 0.0                , 0.0                 , 1.35                , -1.85            , 1.6                , -1.8             , 1.30]</rosparam>
    -->
    <!--
    <rosparam param="start_pose_joint_states">[0.35             , 0.15          , 0.0            , 0.85            , -1.5               , -1.0                , -2.3                , -2.1             , 2.4                , 0.7              , 0.24]
